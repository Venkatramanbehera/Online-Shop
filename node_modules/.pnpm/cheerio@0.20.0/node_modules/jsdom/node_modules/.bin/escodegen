#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/escodegen.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/escodegen@1.14.3/node_modules/escodegen/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/escodegen@1.14.3/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/jsdom@7.2.2/node_modules/jsdom/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/jsdom@7.2.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/escodegen.js" "$@"
else 
  exec node  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/escodegen.js" "$@"
fi
