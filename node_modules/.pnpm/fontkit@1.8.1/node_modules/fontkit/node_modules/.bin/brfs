#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/bin/cmd.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/brfs@2.0.2/node_modules/brfs/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/brfs@2.0.2/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/fontkit@1.8.1/node_modules/fontkit/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/fontkit@1.8.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../brfs@2.0.2/node_modules/brfs/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../brfs@2.0.2/node_modules/brfs/bin/cmd.js" "$@"
fi
