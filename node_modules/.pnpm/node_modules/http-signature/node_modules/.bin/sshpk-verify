#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/bin/sshpk-verify/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/sshpk@1.16.1/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/http-signature@1.2.0/node_modules/http-signature/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/http-signature@1.2.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sshpk@1.16.1/node_modules/sshpk/bin/sshpk-verify" "$@"
else 
  exec node  "$basedir/../../../../../sshpk@1.16.1/node_modules/sshpk/bin/sshpk-verify" "$@"
fi
