#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/quote-stream@1.0.2/node_modules/quote-stream/bin/cmd.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/quote-stream@1.0.2/node_modules/quote-stream/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/quote-stream@1.0.2/node_modules/quote-stream/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/quote-stream@1.0.2/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules/quote-stream/bin/cmd.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules/quote-stream/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules/quote-stream/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../quote-stream/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../quote-stream/bin/cmd.js" "$@"
fi
