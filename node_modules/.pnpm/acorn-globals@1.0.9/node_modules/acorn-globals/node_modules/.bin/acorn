#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/acorn@2.7.0/node_modules/acorn/bin/acorn/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/acorn@2.7.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/acorn@2.7.0/node_modules/acorn/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/acorn@2.7.0/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/acorn-globals@1.0.9/node_modules/acorn-globals/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/acorn-globals@1.0.9/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@2.7.0/node_modules/acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../../../../../acorn@2.7.0/node_modules/acorn/bin/acorn" "$@"
fi
