#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/esprima@1.0.4/node_modules/esprima/bin/esvalidate.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/esprima@1.0.4/node_modules/esprima/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/esprima@1.0.4/node_modules/esprima/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/esprima@1.0.4/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/ast-transform@0.0.0/node_modules/ast-transform/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/ast-transform@0.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@1.0.4/node_modules/esprima/bin/esvalidate.js" "$@"
else 
  exec node  "$basedir/../../../../../esprima@1.0.4/node_modules/esprima/bin/esvalidate.js" "$@"
fi
