#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/semver/bin/semver/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/semver/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/semver/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else 
  exec node  "$basedir/../semver/bin/semver" "$@"
fi
