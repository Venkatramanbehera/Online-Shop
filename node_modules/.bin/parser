#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/@babel/parser/bin/babel-parser.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/@babel/parser/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/@babel/parser/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/@babel/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else 
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
