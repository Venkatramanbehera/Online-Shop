#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/rimraf/bin.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/rimraf/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
