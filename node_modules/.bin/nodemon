#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/bin/nodemon.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/nodemon@2.0.7/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/.pnpm/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules:/mnt/c/Users/asus/Desktop/React/node_modules:/mnt/c/Users/asus/Desktop/node_modules:/mnt/c/Users/asus/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/nodemon/bin/nodemon.js/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/nodemon/bin/node_modules:/mnt/c/Users/asus/Desktop/React/Node/node_modules/nodemon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else 
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
